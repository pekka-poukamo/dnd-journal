{
  "reporter": ["text", "json", "json-summary", "html"],
  "reports-dir": "coverage",
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**",
    "**/*.config.js",
    "**/*.test.js"
  ],
  "include": [
    "js/**/*.js",
    "*.js"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80
}